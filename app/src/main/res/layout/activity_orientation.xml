<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWifiBg"
    android:orientation="vertical"
    tools:context="ywcai.ls.mobileutil.tools.Orientation.OrientationActivity">

    <include layout="@layout/toolbar_head"></include>

    <RelativeLayout
        android:id="@+id/orientation_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="30dp"
        android:background="@color/colorWifiBg"
        android:gravity="center"
        android:minHeight="200dp"></RelativeLayout>
</LinearLayout>
